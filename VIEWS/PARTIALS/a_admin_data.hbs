        <div id="admin_data" class="admin-data" style="display: none;">
            <header>Manage Admin</header>
            
            <a>
                <form action="ADMIN/register" method="post" id="gotoregistrationform">
                    <div class="add-admin" id="gotoregistration">
                        <svg xmlns="http://www.w3.org/2000/svg"
                        height="1em"
                        viewBox="0 0 448 512">
                        <path
                            d="M64 80c-8.8 0-16 7.2-16 16V416c0 8.8 7.2 16 16 16H384c8.8 0 16-7.2 16-16V96c0-8.8-7.2-16-16-16H64zM0 
                            96C0 60.7 28.7 32 64 32H384c35.3 0 64 28.7 64 64V416c0 35.3-28.7 64-64 64H64c-35.3 0-64-28.7-64-64V96zM200 
                            344V280H136c-13.3 0-24-10.7-24-24s10.7-24 24-24h64V168c0-13.3 10.7-24 24-24s24 10.7 24 24v64h64c13.3 0 24 10.7 
                            24 24s-10.7 24-24 24H248v64c0 13.3-10.7 24-24 24s-24-10.7-24-24z"
                        />
                        </svg>
                        Add Admin
                    </div>
                    <script>
                        let gotoregistration = document.getElementById('gotoregistration');
                        let gotoregistrationform = document.getElementById('gotoregistrationform');
                        gotoregistration.addEventListener('click', () => {
                            gotoregistrationform.submit();
                        });
                    </script>
                </form>
            </a>
            <table>
                <tr>
                    <th>No</th>
                    <th>Name</th>
                    <th>Gmail</th>
                    <th>Phone</th>
                    <th>Add Date</th>
                    <th>Action</th>
                </tr>
                {{#each UsersData}}
                    <tr>
                        <td>
                            {{this.id}}
                        </td>
                        <td>
                            {{this.name}}
                        </td>
                        <td>
                            {{this.email}}
                        </td>
                        <td>
                            {{this.phone}}
                        </td>
                        <td>
                            {{this.add_time}}
                        </td>
                        <td>
                                <form action="admin/delete-user" method="post">
                                    <input type="hidden" name="admin_id" value="{{this.id}}">
                                    <button class="remove_btn" type="submit" name="removeButton{{this.id}}">Remove</button>
                                </form>
                        </td>
                    </tr>
                {{/each}}
            </table>
        </div>